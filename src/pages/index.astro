---
import Layout from '../layouts/Layout.astro';
import Summary from '../components/Summary.jsx';
import Result from '../components/Result.jsx';
import data from '../assets/data.json'
---

<Layout title="Results Summary Component">
	<main class="h-full lg:w-full md:flex justify-center items-center">
		<div class="md:flex md:flex-row lg:w-1/2 md:shadow-lg md:rounded-3xl">
			<Result result={data} client:load/>
			<Summary />
		</div>
	</main>
</Layout>